{
  "name": "egreti18n",
  "displayName": "egreti18n",
  "description": "",
  "version": "0.0.2",
  "publisher": "ok",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.EgretI18nSave",
    "onCommand:extension.EgretI18nUse",
    "onCommand:extension.EgretI18nExportChinese"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.EgretI18nSave",
        "title": "EgretI18n save",
        "category": "保存.exml内容至resource/i18n(覆盖)： "
      },
      {
        "command": "extension.EgretI18nUse",
        "title": "EgretI18n use",
        "category": "切换语言（合并）： "
      },
      {
        "command": "extension.EgretI18nExportChinese",
        "title": "EgretI18n ExportChinese",
        "category": "提取皮肤文件中文至exml.json： "
      }
    ],
    "configuration": {
      "type": "object",
      "title": "EgretI18n",
      "properties": {
        "EgretI18n.skinPath": {
          "type": "string",
          "items": {
            "type": "string"
          },
          "markdownDescription": "皮肤文件路径",
          "scope": "window",
          "default": "resource/skins"
        },
        "EgretI18n.exportTags": {
          "type": "array",
          "items": {
            "type": "array"
          },
          "markdownDescription": "导出标签数组",
          "scope": "window",
          "default": [
            [
              "e:Label",
              "text"
            ],
            [
              "e:Label",
              "text.up"
            ],
            [
              "e:Label",
              "text.down"
            ],
            [
              "e:Label",
              "text.disabled"
            ],
            [
              "e:Label",
              "text.unOpen"
            ],
            [
              "e:BitmapLabel",
              "text"
            ],
            [
              "e:Button",
              "label"
            ],
            [
              "e:EditableText",
              "prompt"
            ],
            [
              "e:EditableText",
              "text"
            ],
            [
              "e:TextInput",
              "prompt"
            ]
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.43.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "uuid": "^7.0.2"
  }
}
